{
    "servers": [
      {
        "name": "Obsidian",
        "type": "stdio",
        "command": ["uvx", "mcp-obsidian"],
        "env": {
          "OBSIDIAN_API_KEY": "<your_api_key_here>"
        }
      },
      {
        "name": "GitHub",
        "type": "stdio",
        "command": ["npx", "-y", "@modelcontextprotocol/server-github"],
        "env": {
          "GITHUB_PERSONAL_ACCESS_TOKEN": "<YOUR_TOKEN>"
        }
      }
    ]
  }